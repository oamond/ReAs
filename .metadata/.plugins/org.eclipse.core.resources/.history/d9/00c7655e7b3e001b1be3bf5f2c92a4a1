
public class Assignment2 {

	public static void main(String[] args) {
		
		String list1 = "ABC";
		String list2 = "abc";
		
		if (list1 == list2) {
			System.out.println("String list1 and list2 are identical");
		} else {
			System.out.println("String list1 and list2 are not identical");
		}
		

	}

}
